{
	"name": "ultrahope",
	"version": "0.1.0",
	"description": "LLM-powered development workflow assistant",
	"type": "module",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/toyamarinyon/ultrahope.git"
	},
	"keywords": [
		"cli",
		"llm",
		"ai",
		"git",
		"commit-message",
		"pull-request"
	],
	"bin": {
		"ultrahope": "dist/index.js",
		"git-ultrahope": "dist/git-ultrahope.js",
		"git-hope": "dist/git-ultrahope.js",
		"git-uh": "dist/git-ultrahope.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"generate:client": "openapi-typescript ../web/openapi.json -o lib/api-client.generated.ts",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "pnpm run build"
	},
	"dependencies": {
		"open": "^11.0.0",
		"openapi-fetch": "^0.15.0"
	},
	"devDependencies": {
		"@types/node": "^22.15.29",
		"openapi-typescript": "^7.10.1",
		"tsup": "8.5.0",
		"typescript": "^5.8.3"
	}
}
